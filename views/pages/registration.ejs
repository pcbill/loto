<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

  <% include ../partials/nav.ejs %>

<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h3>
      Registration
      </h3>

      <form action="/registerSubmit" method="POST">
      uid <input name="uid" type="text" />
        <button type="submit" value="submit">submit</button>
      </form>
      <br/>
      <% if (results) { %>
        <br/>
        There are <%= results.length %> people registered
        <ul>
	      <% results.forEach( (it) => { %>
		      <li><a href='/deleteRegistration/<%= it.id %>'>del</a><%= it.id %>, <%= it.uid %>, <%= it.name %>, <%= it.registration_time %></li>
	      <% }); %>
	      </ul>
      <% } %>
    </div>
  </div>
</div>


</body>
</html>
